{
	"name": "df_TitleMaster_csv_to_sql",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_nycpayroll_TitleMaster_csv",
						"type": "DatasetReference"
					},
					"name": "sourceTitlecsv"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_NYC_Payroll_TITLE_MD_sql",
						"type": "DatasetReference"
					},
					"name": "sinkTitlesql"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceTitlecsv",
				"sourceTitlecsv sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          TitleCode,",
				"          TitleDescription",
				"     )) ~> sinkTitlesql"
			]
		}
	}
}